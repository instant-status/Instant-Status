{
  "compilerOptions": {
    "rootDirs": [
      "../config/",
      "src",
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "composite": true,
    "outDir": "dist"

  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "**/*.spec.ts"],
  "lib": ["es2020"]
}
