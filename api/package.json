{
  "name": "instant-status-api",
  "version": "1.0.0",
  "description": "API",
  "main": "src/server.ts",
  "scripts": {
    "dev": "dotenv -e ../config/.prisma.env -- nodemon --watch src",
    "start": "dotenv -e ../config/.prisma.env -- ts-node src/server.ts",
    "db:dev-push": "dotenv -e ../config/.prisma.env -- prisma db push",
    "db:dev-migrate": "dotenv -e ../config/.prisma.env -- prisma migrate dev",
    "db:dev-seed": "dotenv -e ../config/.prisma.env -- prisma db seed --preview-feature",
    "db:deploy": "dotenv -e ../config/.prisma.env -- prisma migrate deploy",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instant-status/instant-status.git"
  },
  "author": "Instant Status",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instant-status/instant-status/issues"
  },
  "homepage": "https://github.com/instant-status/instant-status#readme",
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "10.1.0",
    "@prisma/client": "2.28.0",
    "googleapis": "84.0.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.1",
    "koa-bodyparser": "4.3.0",
    "koa-google-sign-in": "2.0.3",
    "koa-json": "2.0.2",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.4",
    "@types/koa": "2.13.4",
    "@types/koa-bodyparser": "4.3.3",
    "@types/koa-json": "2.0.20",
    "@types/koa__cors": "3.0.3",
    "@types/koa__router": "8.0.7",
    "@typescript-eslint/parser": "4.29.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "prisma": "2.28.0",
    "ts-node": "10.1.0",
    "ts-node-dev": "1.1.8",
    "type-fest": "2.0.0"
  }
}
