{
  "name": "instant-status-api",
  "version": "1.0.0",
  "description": "API",
  "main": "src/server.ts",
  "scripts": {
    "dev": "dotenv -e ../config/.prisma.env -- nodemon --watch src",
    "start": "dotenv -e ../config/.prisma.env -- ts-node src/server.ts",
    "db:dev-generate-migrations": "dotenv -e ../config/.prisma.env -- prisma migrate dev --create-only",
    "db:dev-push": "dotenv -e ../config/.prisma.env -- prisma db push",
    "db:dev-reset": "dotenv -e ../config/.prisma.env -- prisma migrate reset",
    "db:dev-seed": "dotenv -e ../config/.prisma.env -- prisma db seed",
    "db:dev-studio": "dotenv -e ../config/.prisma.env -- prisma studio",
    "db:migrate": "dotenv -e ../config/.prisma.env -- prisma migrate deploy",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "tsc": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instant-status/instant-status.git"
  },
  "author": "Instant Status",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instant-status/instant-status/issues"
  },
  "homepage": "https://github.com/instant-status/instant-status#readme",
  "dependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "10.1.1",
    "@prisma/client": "3.9.2",
    "googleapis": "95.0.0",
    "jsonwebtoken": "8.5.1",
    "koa": "2.13.4",
    "koa-bodyparser": "4.3.0",
    "koa-google-sign-in": "2.0.3",
    "koa-json": "2.0.2",
    "typescript": "4.5.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "8.5.8",
    "@types/koa": "2.13.4",
    "@types/koa__cors": "3.1.1",
    "@types/koa__router": "8.0.11",
    "@types/koa-bodyparser": "4.3.5",
    "@types/koa-json": "2.0.20",
    "@typescript-eslint/parser": "5.11.0",
    "dotenv-cli": "5.0.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "prisma": "3.9.2",
    "ts-node": "10.5.0",
    "ts-node-dev": "1.1.8",
    "type-fest": "2.12.0"
  }
}
